# Staging Environment Configuration
NODE_ENV=staging
PORT=3000

# Database Configuration
DATABASE_URL=postgresql://mindnote:password@staging-db.example.com:5432/mindnote_staging
POSTGRES_USER=mindnote
POSTGRES_PASSWORD=password
POSTGRES_DB=mindnote_staging

# Redis Configuration
REDIS_URL=redis://staging-redis.example.com:6379
REDIS_PASSWORD=redis_staging_password

# Authentication Configuration
NEXTAUTH_URL=https://staging.mindnote.com
NEXTAUTH_SECRET=staging-secret-key-at-least-32-characters-long

# AI Service Configuration
OPENAI_API_KEY=your-staging-openai-api-key
ANTHROPIC_API_KEY=your-staging-anthropic-api-key
AI_MODEL_DEFAULT=gpt-3.5-turbo
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2048
AI_TIMEOUT=30000

# Monitoring and Logging
LOG_LEVEL=info
LOG_FORMAT=json
ENABLE_MONITORING=true

# Feature Flags
ENABLE_AI_FEATURES=true
ENABLE_ANALYTICS=true
ENABLE_DEBUG_MODE=false

# Performance Configuration
ENABLE_CACHING=true
CACHE_TTL=3600
ENABLE_COMPRESSION=true

# Security Configuration
CORS_ORIGIN=https://staging.mindnote.com
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=900000

# External Services
WEBHOOK_URL=https://staging.mindnote.com/api/webhooks
EMAIL_FROM=noreply@staging.mindnote.com